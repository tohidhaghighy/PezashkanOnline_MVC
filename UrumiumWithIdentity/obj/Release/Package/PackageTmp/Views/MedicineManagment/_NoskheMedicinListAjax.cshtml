@model List<UrumiumMVC.DomainClasses.Entities.Medicine.Medicine>
@{
    if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td class="text-center" style="padding:10px;">@item.Name</td>
                <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="درج نسخه"><a class="btn btn-success btn-xs" onclick="modalshowaddmedicinetonoskhe('@item.Name');"><i class="ion ion-ios-albums"></i></a></p></td>
            </tr>
        }
    }


}
