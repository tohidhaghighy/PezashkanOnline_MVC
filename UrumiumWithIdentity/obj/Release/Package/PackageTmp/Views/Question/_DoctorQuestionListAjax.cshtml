@model List<UrumiumMVC.DomainClasses.Entities.Question.Question>
@{
    foreach (var item in Model)
    {
        <tr>

            <td class="text-center" style="padding:10px;">@item.Text</td>
            <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="ویرایش"><a class="btn btn-warning btn-xs" onclick="Updateitem(@item.Id,'@item.Text')"><i class="ion ion-ios-contact"></i></a></p></td>
            <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-danger btn-xs" onclick="deleteitem(@item.Id)"><i class="ion ion-ios-trash"></i></a></p></td>
        </tr>
    }

}
