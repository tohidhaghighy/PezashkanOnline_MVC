    {
        <tr>
            <td class="text-center" style="padding:10px;">@item.Insurances.Name</td>
            
            <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-danger btn-xs" onclick="deletebimeitem(@item.Id,@item.DoctorId)"><i class="ion ion-ios-trash"></i></a></p></td>
        </tr>
    }

}
